{
  "name": "@nexwave/cli",
  "version": "0.1.0-alpha.1",
  "description": "Command-line interface for Nexwave execution kernel",
  "type": "module",
  "bin": {
    "nexwave": "./dist/bin/nexwave.js"
  },
  "files": [
    "dist",
    "templates",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/nexwave-io/nexwave-ts.git",
    "directory": "packages/cli"
  },
  "homepage": "https://nexwave.so",
  "bugs": {
    "url": "https://github.com/nexwave-io/nexwave-ts/issues"
  },
  "author": "Nexwave",
  "license": "MIT",
  "keywords": [
    "nexwave",
    "defi",
    "solana",
    "trading",
    "cli",
    "execution",
    "command-line"
  ],
  "scripts": {
    "dev": "bun run bin/nexwave.ts",
    "build": "bun build bin/nexwave.ts --outdir ./dist/bin --target node && tsc --emitDeclarationOnly",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "lint": "eslint src/"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.30.0",
    "@nexwave/sdk": "workspace:*",
    "@nexwave/types": "workspace:*",
    "chalk": "^5.3.0",
    "commander": "^12.0.0",
    "conf": "^12.0.0",
    "ink": "^5.0.0",
    "ink-spinner": "^5.0.0",
    "ink-table": "^3.1.0",
    "ink-text-input": "^6.0.0",
    "keytar": "^7.9.0",
    "ora": "^8.0.0",
    "prompts": "^2.4.2",
    "react": "^18.2.0",
    "react-devtools-core": "^7.0.1",
    "yaml": "^2.3.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/node": "^20.0.0",
    "@types/prompts": "^2.4.0",
    "@types/react": "^18.2.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}

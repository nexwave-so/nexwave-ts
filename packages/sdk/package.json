{
  "name": "@nexwave/sdk",
  "version": "0.1.0-alpha.1",
  "description": "TypeScript SDK for Nexwave - deterministic DeFi execution",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/nexwave-io/nexwave-ts.git",
    "directory": "packages/sdk"
  },
  "homepage": "https://github.com/nexwave-io/nexwave-ts/tree/main/packages/sdk",
  "bugs": {
    "url": "https://github.com/nexwave-io/nexwave-ts/issues"
  },
  "author": "Nexwave <hello@nexwave.io>",
  "license": "MIT",
  "keywords": [
    "nexwave",
    "defi",
    "solana",
    "intent",
    "execution",
    "trading",
    "sdk",
    "typescript",
    "client"
  ],
  "scripts": {
    "build": "bun build ./src/index.ts --outdir ./dist --target node && tsc --emitDeclarationOnly",
    "test": "bun test",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "bun run build"
  },
  "dependencies": {
    "@nexwave/types": "workspace:*",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "typescript": "^5.3.0"
  },
  "peerDependencies": {
    "typescript": ">=5.0.0"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}

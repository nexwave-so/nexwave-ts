{
  "name": "@nexwave/types",
  "version": "0.1.0-alpha.1",
  "description": "Type definitions and Zod schemas for Nexwave Runtime API",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/nexwave-io/nexwave-ts.git",
    "directory": "packages/types"
  },
  "homepage": "https://github.com/nexwave-io/nexwave-ts/tree/main/packages/types",
  "bugs": {
    "url": "https://github.com/nexwave-io/nexwave-ts/issues"
  },
  "author": "Nexwave <hello@nexwave.io>",
  "license": "MIT",
  "keywords": [
    "nexwave",
    "types",
    "typescript",
    "zod",
    "defi",
    "intent"
  ],
  "scripts": {
    "build": "bun build ./src/index.ts --outdir ./dist --target node && tsc --emitDeclarationOnly",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "bun run build"
  },
  "dependencies": {
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
